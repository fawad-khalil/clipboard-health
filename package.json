{
  "name": "fawad-khalil",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "./node_modules/.bin/ts-node src/bin/www",
    "start:dev": "docker-compose up --build",
    "start:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "migrate": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run -d ormconfig/ormconfig.ts",
    "migrate:revert": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:revert -d ormconfig/ormconfig.ts",
    "seed": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm-extension seed -d ormconfig/ormconfig.ts",
    "migrate:dev": "NODE_ENV=development yarn migrate",
    "migrate:prod": "NODE_ENV=production yarn migrate",
    "migrate:test": "NODE_ENV=test yarn migrate",
    "migrate:revert:dev": "NODE_ENV=development yarn migrate:revert",
    "migrate:revert:prod": "NODE_ENV=development yarn migrate:revert",
    "migrate:revert:test": "NODE_ENV=development yarn migrate:revert",
    "seed:dev": "NODE_ENV=development yarn seed",
    "seed:prod": "NODE_ENV=production yarn seed",
    "seed:test": "NODE_ENV=test yarn seed",
    "test": "jest"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/hapi__joi": "^17.1.9",
    "class-validator": "^0.14.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.9.1",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.11",
    "typeorm-extension": "^2.4.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/jest": "^29.2.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.20",
    "jest": "^29.4.0",
    "redis-mock": "^0.56.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
